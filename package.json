{
  "name": "treecko",
  "version": "3.0.0",
  "description": "Functional tree helpers",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist --ignore test.js",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "prebuild": "yarn run clean -s",
    "prerelease": "yarn run lint -s && yarn run testonce -s && yarn run build -s",
    "release": "yarn publish",
    "test": "ava --watch",
    "testonce": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickjohnson-dev/treecko.git"
  },
  "author": {
    "name": "Nick Johnson",
    "email": "nickjohnson.dev@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickjohnson-dev/treecko/issues"
  },
  "homepage": "https://github.com/nickjohnson-dev/treecko#readme",
  "devDependencies": {
    "ava": "0.18.1",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.0",
    "babel-eslint": "7.1.1",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-es2017": "6.22.0",
    "babel-preset-stage-3": "6.22.0",
    "eslint": "3.15.0",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-lodash-fp": "2.1.3",
    "sinon": "^2.2.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "lodash": "4.17.4"
  }
}
